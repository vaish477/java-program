import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
          Scanner sc = new Scanner(System.in);

        int n = sc.nextInt();

        for (int i = 1; i <= n; i++) {
            
            char startChar = (char) ('A' + i - 1);

            for (int j = 0; j < i; j++) {
                System.out.print((char)(startChar + j) + " ");
            }
            System.out.println();
        }
    }
}
